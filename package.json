{
  "name": "fork-knife-backend",
  "version": "1.0.0",
  "description": "Fork & Knife Restaurant Booking System - Production Ready Modular Monolith",
  "author": "Fork & Knife Team",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=18 <21",
    "npm": ">=9"
  },
  "scripts": {
    "build": "npx nest build",
    "start": "npx nest start",
    "start:dev": "npx nest start --watch",
    "start:debug": "npx nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:worker": "node dist/worker/main",
    "lint": "npx eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "npx prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:cov": "npx jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npx jest --config ./test/jest-e2e.json",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate:dev": "npx prisma migrate dev",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "npx ts-node prisma/seed.ts",
    "prisma:reset": "npx prisma migrate reset",
    "check:env": "npx ts-node scripts/check-env.ts",
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/throttler": "^5.1.1",
    "@prisma/client": "^5.8.0",
    "@sentry/node": "^7.120.4",
    "@sentry/profiling-node": "^7.120.4",
    "@supabase/supabase-js": "^2.39.0",
    "@types/compression": "^1.8.1",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "date-fns": "^3.0.6",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^17.2.3",
    "graphql": "^16.8.1",
    "graphql-depth-limit": "^1.1.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "nestjs-pino": "^3.5.0",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^8.17.2",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.1",
    "stripe": "^14.10.0",
    "twilio": "^4.20.0",
    "@nestjs/cli": "^10.3.0",
    "prisma": "^5.8.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.38",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@modules/(.*)$": "<rootDir>/modules/$1",
      "^@common/(.*)$": "<rootDir>/common/$1",
      "^@config/(.*)$": "<rootDir>/config/$1"
    },
    "coverageThreshold": {
      "global": {
        "lines": 70,
        "branches": 60,
        "functions": 70,
        "statements": 70
      }
    }
  }
}
